<!DOCTYPE html>
<html>
<head>
  <title>Seatyourself</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<header>
<% if current_user %>
  <%= current_user.email %>
  <%= link_to "Log out", session_path("current"), :method => :delete %>
<% else%>
  <%= link_to "Sign Up", new_user_url %>
  <%= link_to "Sign In", new_session_url %>
<% end %>
</header>

<%= yield %>

</body>
</html>
